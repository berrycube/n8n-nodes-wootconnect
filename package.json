{
  "name": "n8n-suite",
  "version": "0.1.0",
  "private": true,
  "description": "n8n suite: public plugins + private integration + commercial extensions",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.15",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lintfix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "dev": "pnpm -r --parallel run dev",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "bootstrap": "pnpm install && pnpm build",
    "check:types": "pnpm -r run typecheck",
    "check:security": "pnpm audit --audit-level=high && pnpm -r run security-scan",
    "check:all": "pnpm run check:types && pnpm run lint && pnpm run format:check && pnpm run test"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^3.2.4",
    "concurrently": "^8.2.2"
  },
  "workspaces": ["public/*", "private/*", "private/n8n-pro-extensions/packages/*"],
  "pnpm": {
    "overrides": {
      "form-data": ">=4.0.4",
      "n8n-workflow": "^1.106.0",
      "typescript": "^5.3.3",
      "vitest": "^3.2.4",
      "@typescript-eslint/eslint-plugin": "^8.41.0",
      "@typescript-eslint/parser": "^8.41.0",
      "@vitest/ui": "^3.2.4"
    },
    "patchedDependencies": {},
    "peerDependencyRules": {
      "ignoreMissing": ["n8n-workflow"]
    }
  }
}